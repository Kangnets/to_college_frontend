{
  "rewrites": [
    {
      "source": "/ph-x9a/:path*",
      "destination": "https://us.i.posthog.com/:path*",
      "headers": {
        "Host": "us.i.posthog.com"
      }
    },
    {
      "source": "/ph-x9a/static/:path*",
      "destination": "https://us-assets.i.posthog.com/static/:path*",
      "headers": {
        "Host": "us-assets.i.posthog.com"
      }
    }
  ]
}
